  <Style name="test">
    <Rule>
      <TextSymbolizer face-name="Khmer OS Regular" size="16" placement="point">[name]</TextSymbolizer>
    </Rule>  
  </Style>

  <Layer name="lay" srs="+proj=longlat +ellps=airy +datum=OSGB36 +no_defs ">
